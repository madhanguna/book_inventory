<h1>User Details</h1>

<table border=1>
  <tr>
  		<th>Photo</th>
  		<th>EmailID</th>
  </tr>
 
  <% @user.each do |u| %>
    <tr>
    	<td><%= image_tag avatar_url(u) %></td>
      	<td><%= u.email %></td>
	      <td><%= link_to 'Show', user_path(u) %></td>
       

         
    </tr>
  <% end %>

</table>
